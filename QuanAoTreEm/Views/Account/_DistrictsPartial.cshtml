@model List<QuanAoTreEm.Models.district>

<label for="district" class="col-sm-2 col-form-label">Quận/ huyện</label>
<div class="col-sm-10">
    <select class="form-control" id="district" name="district" placeholder="Nhập để tìm kiếm" required onchange="loadWards()">
        @if (Model != null)
        {
            foreach (var district in Model)
            {
                <option value="@district.code">@district.name</option>
            }
        }
    </select>
</div>